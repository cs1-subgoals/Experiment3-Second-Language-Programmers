          <exercise xml:id="survey-Q5">
            <statement>
              <p>
                Q5: Based on your previous CS experience, how much do you agree or disagree with the following statement? <strong>[ONLY USED FOR STUDENTS IN CSE 115 &amp; 116]</strong>
              </p>
              <p>
                <em>I am better at computer science than most of the other students in my course.</em>
              </p>
            </statement>
          
            <choices>
              <choice correct="yes">
                <statement><p>1 - Completely Disagree</p></statement>
              </choice>
              <choice correct="yes">
                <statement><p>2 - Mostly Disagree</p></statement>
              </choice>
              <choice correct="yes">
                <statement><p>3 - Slightly Disagree</p></statement>
              </choice>
              <choice correct="yes">
                <statement><p>4 - Slightly Agree</p></statement>
              </choice>
              <choice correct="yes">
                <statement><p>5 - Mostly Agree</p></statement>
              </choice>
              <choice correct="yes">
                <statement><p>6 - Completely Agree</p></statement>
              </choice>
              <choice correct="yes">
                <statement><p>Not a student of CSE 115 or 116.</p></statement>
              </choice>
            </choices>
          </exercise>
          
